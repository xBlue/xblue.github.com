.sR{width:422px; height:257px; float:right; display:inline; background:url(../images/sortBg.gif) no-repeat 0 0;}

.gg{overflow:hidden; zoom:1; padding-top:10px;}
.gg .g-L{ float:left; background:url(../images/caldate_Bg.gif) left top no-repeat; height:69px; width:71px; overflow:hidden;}
.gg .g-L .g-num{ width:56px; height:53px; font-family:"Arial Black"; text-align:center; display:block; line-height:53px; font-size:36px; color:#463001; clear:both;}
.gg .g-L .g-txt{ margin:0 auto; height:16px; line-height:18px; text-align:center;clear:both; color:#D8C19D; overflow:hidden;}
.gg .g-L .g-txt a{color:#D8C19D;}
.gg .g-R{float:right; display:inline; width:200px; line-height:22px;}

.dateList li{ height:24px; line-height:24px; overflow:hidden;}
.dateList span{ background:url(../images/bg_x.gif) left -155px repeat-x; height:20px; width:61px; padding-right:8px; margin-right:9px; text-align:right; line-height:20px; display:inline-block; *display:inline; zoom:1; border:1px solid #D7D7D7;}

.idrList{overflow:hidden; border:1px #DCDDDD solid; border-top:none;}
.idrList li {background:url(../images/bg_x.gif) left -124px repeat-x; height:37px; line-height:37px; overflow:hidden; zoom:1; padding:0 10px;}
.idrList .idrIcon_i {float:left; background:url(../images/icon_i.gif) right center no-repeat; height:19px; line-height:19px; display:inline-block; *display:inline; zoom:1; margin:9px 0px 0px 0px; padding-right:20px;}
.idrList .idrIcon_add {float:right; background:url(../images/icon_add.gif) left top no-repeat; width:55px; height:17px; display:block; margin:9px 0px 0px 2px;cursor:pointer;}
.wbIcon_add { background:url(../images/icon_add.gif) left top no-repeat; width:55px; height:17px; display:inline-block; *display:inline; zoom:1; cursor:pointer;}

.fL{float:left; display:inline; width:473px;}
.fR{float:right; display:inline; width:473px;}
.fList .imgList{width:230px;}

.logo{overflow:hidden; zoom:1; padding:10px 0;}
.logo li{float:left; display:inline; padding:10px 23px 0;}

.rightBox4{border:1px solid #DACFB5; overflow:hidden;}
.rightTitle{background:#F6F2EA; height:32px; line-height:32px; margin:0 auto; color:#776352;}
.rightTitle h2{float:left; padding:0 3px 0 11px; font-weight:700; font-size:14px;}
.rightTitle a{color:#776352;}

.channel-end,.channelEnd-rightS,.channelEnd-link,.channelEnd-top {background:url(../images/endnav_bg1001.jpg) no-repeat;}
.channel-end {height:37px;overflow:hidden;margin:10px auto 0;line-height:44px;background-position:left -67px; background-repeat:repeat-x;}
.channelEnd-rightS {display:block;float:right;width:8px;height:37px;overflow:hidden;background-position:right -141px;}
.channelEnd-link {float:left;height:37px;overflow:hidden;color:#1E50A2;background-position:left -30px;}
.channelEnd-link a {float:none;padding:0 5px;font-weight:normal;background:none;}
.channelEnd-link .firstNode {margin-left:5px; background:none;}
.channelEnd-top {display:block;float:right;width:70px;height:37px;overflow:hidden;text-indent:10px;background-position:left -30px;}

.listeditor{width:960px; margin:0 auto; line-height:21px;clear:both;color:#4D4D4D;line-height:21px;padding:9px 0px 3px; text-align:center;}

.design{ width:422px; height:257px; overflow:hidden;}
.design h3{font-size:14px; color:#81511c; line-height:25px; font-weight:700;}
.design-1,.design-2,.design-3{float:left; padding-top:3px; overflow:hidden;}
.design-1{width:80px; padding-left:4px; padding-right:1px;}
.design-3 ul{height:195px; overflow:hidden; overflow-y:auto; width:200px;}
.design li{height:25px; line-height:25px; padding-left:8px; color:#2b2b2b; cursor:pointer; overflow:hidden;}
.design-3 ul li a{width:175px; height:25px; padding-left:8px; line-height:25px; display:inline-block; color:#2b2b2b;}
.design-3 ul li a:hover{color:#ba2636; background:#e7dac6;}
.design-1 ul li.on,.design-2 ul li.on{background:#e7dac6; font-weight:bold;}
.design-2{width:118px; padding-left:4px; padding-right:3px;}
.design-3{width:200px; padding-left:8px;}
.design p{float:right; line-height:30px; color:#81511c;}

.foot{margin:0 auto; width:960px;line-height:21px; clear:both; text-align:center;}
.foot .text{padding:5px 0 4px; border-bottom:1px solid #dcdddd; margin:0 0 6px;}