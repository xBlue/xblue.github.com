@charset "utf-8";
/* CSS reset */
body,div,h1,h2,h3,h4,h5,h6,hr,p,blockquote,dl,dt,dd,ul,ol,li,pre,code,form,fieldset,legend,button,input,textarea,th,td{margin:0;padding:0;}
body,input,button,textarea,select,option,optgroup{font:12px/1 \5b8b\4f53,sans-serif}
h1,h2,h3,h4,h5,h6 {font-size:12px; font-weight:normal;} 
menu,ol,ul{list-style:none}
table{border-collapse:collapse;border-spacing:0}
fieldset,img{border:none;}
legend{display:none;}
a:active,a:focus{outline: none;}
address,cite,dfn,em,var{font-style:normal}
code,kbd,pre,samp{font-family:'courier new',courier,monospace}
input,button,textarea,select{font-size:100%}
abbr[title]{border-bottom:1px dotted;cursor:help}
a{text-decoration:none;outline:none;color:#2B2B2B;}
.clearfix:after{visibility:hidden;display:block;font-size:0;content:"\0020";clear:both;height:0;}
.clearfix{*zoom:1;}
.blank10{width:100%; height:10px; overflow:hidden; clear:both;}
.blank15{width:100%; height:15px; overflow:hidden; clear:both;}
.blank20{width:100%; height:20px; overflow:hidden; clear:both;}
.linkRed{color:#F00;}
.font18{font-size:18px;}
.font12,.rightBox p.font12{font-size:12px; font-weight:normal;}
.cBlue a,#top .icon a{color:#1E50A2;}

body{background:#E7DAC6;}
#wrap{width:960px; background:#fff; margin:0 auto; overflow:hidden; padding:0 10px;}

#top a:hover,#ads a:hover,#channel p a:hover,.part a:hover{color:#ba2636; text-decoration:underline;}
#ads ul,#channel_nav,.part,.title,.imgList li,.txtList2,.inList,.fList{overflow:hidden; zoom:1;}
